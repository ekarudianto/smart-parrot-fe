    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-preset-env": "^1.5.2",
    "chalk": "^2.4.1",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-vue": "^4.3.0",
    "expect": "^22.4.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.3",
    "jest-serializer-vue": "^1.0.0",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "node-sass": "^4.9.0",
