{
  "name": "vue2_starter",
  "version": "1.0.0",
  "description": "Vuejs 2 starter template",
  "main": "src/main.js",
  "author": "Eka Rudianto",
  "license": "MIT",
  "scripts": {
    "prestart": "yarn install --production=false",
    "build": "webpack",
    "serve": "webpack-dev-server",
    "start": "webpack-dev-server --port $PORT",
    "test:unit": "test=unit jest ./test/unit -u --coverage",
    "test:e2e": "test=e2e jest ./test/e2e -u --coverage",
    "test": "yarn test:unit && yarn test:e2e"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.12",
    "babel-core": "^6.25.0",
    "babel-jest": "^22.4.3",
